{
  "name": "test",
  "version": "1.5.0",
  "private": true,
  "scripts": {
    "start:dev": "pnpm i && pm2-runtime ./config/app.config.js --env development ",
    "start:prod": "pnpm i && pm2-runtime ./config/app.config.js --env production ",
    "stop": "pm2 stop ./config/app.config.js",
    "deploy": "pm2 deploy ./config/app.config.js production update",
    "delete": "pm2 delete ./config/app.config.js",
    "seed": "node src/tasks/seed/",
    "build": "esbuild --bundle app.js --outdir=dist --minify --sourcemap --platform=node",
    "preinstall": "npx only-allow pnpm"
  },
  "dependencies": {
    "bcrypt": "^5.0.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
