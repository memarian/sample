    "express-status-monitor": "^1.3.3",
    "fcm-node": "^1.5.2",
    "formidable": "^1.2.2",
    "helmet": "^4.1.1",
    "http-errors": "^1.8.0",
    "joi": "^17.2.1",
    "joi-objectid": "^3.0.1",
    "jsonwebtoken": "^8.5.1",
    "log-symbols": "^4.0.0",
    "mongoose": "^5.10.6",
    "mongoose-paginate": "^5.0.3",
    "mongoose-unique-validator": "^2.0.3",
    "morgan": "^1.10.0",
    "node-pushnotifications": "^1.4.1",
    "passport": "^0.4.1",
